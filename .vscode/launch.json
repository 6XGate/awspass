{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Show help",
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/awspass.cjs",
      "args": ["--help"],
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Setup profile",
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/awspass.cjs",
      "args": ["setup"],
      "outFiles": ["${workspaceFolder}/**/*.js"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Get session",
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/awspass.cjs",
      "args": ["session"],
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
